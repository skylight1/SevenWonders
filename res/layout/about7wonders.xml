<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
<TextView android:text="About 7 Wonders" 
          android:id="@+id/TextView01"
          android:layout_centerHorizontal="true"
          android:layout_width="fill_parent" 
          android:layout_height="wrap_content"></TextView>
<TextView android:layout_width="wrap_content" 
          android:layout_below="@+id/TextView01"
          android:paddingTop="10sp" 
          android:layout_centerHorizontal="true" 
          android:id="@+id/TextView02" 
          android:layout_height="wrap_content" 
          android:text="7 Wonders was created by"></TextView>
<TextView android:layout_width="wrap_content" 
		  android:layout_below="@+id/TextView02" 
		  android:paddingTop="5sp"
		  android:layout_centerHorizontal="true" 
		  android:id="@+id/TextView03" 
		  android:layout_height="wrap_content" 
		  android:text="the NYC Java Meetup Mobile Group"></TextView>
<TextView android:layout_width="wrap_content" 
		  android:layout_below="@+id/TextView03"
		  android:paddingTop="10sp" 
		  android:layout_centerHorizontal="true" 
		  android:id="@+id/TextView04" 
		  android:layout_height="wrap_content" 
		  android:text="Original Concept by Timothy Fagan,"></TextView>
<TextView android:layout_width="wrap_content" 
          android:layout_below="@+id/TextView04"
          android:paddingTop="5sp" 
          android:layout_centerHorizontal="true" 
          android:id="@+id/TextView05" 
          android:layout_height="wrap_content" 
          android:text="with contributions by:"></TextView>
<TextView android:layout_width="wrap_content" 
	      android:layout_below="@+id/TextView05"
	      android:paddingTop="5sp" 
	      android:layout_centerHorizontal="true" 
	      android:id="@+id/TextView06" 
	      android:layout_height="wrap_content" 
	      android:text="Dario Laverde, Morrison Chang,"></TextView>
<TextView android:layout_width="wrap_content" 
          android:layout_below="@+id/TextView06"
          android:paddingTop="3sp" 
          android:layout_centerHorizontal="true" 
          android:id="@+id/TextView07" 
          android:layout_height="wrap_content" 
          android:text="Lance Nanek, Juan Handal,"></TextView>
<TextView android:layout_width="wrap_content" 
          android:layout_below="@+id/TextView07"
          android:paddingTop="3sp" 
          android:layout_centerHorizontal="true" 
          android:id="@+id/TextView08" 
          android:layout_height="wrap_content" 
          android:text="Mike Mellinger, Johannes Kramer,"></TextView>
<TextView android:layout_width="wrap_content" 
          android:layout_below="@+id/TextView08"
          android:paddingTop="3sp" 
          android:layout_centerHorizontal="true" 
          android:id="@+id/TextView09" 
          android:layout_height="wrap_content" 
          android:text="Cynthia Tian, Gautam Borgohain,"></TextView>
<TextView android:layout_width="wrap_content" 
          android:layout_below="@+id/TextView09"
          android:paddingTop="3sp" 
          android:layout_centerHorizontal="true" 
          android:id="@+id/TextView10" 
          android:layout_height="wrap_content" 
          android:text="and Gregory Dempsey"></TextView>
<Button android:layout_width="wrap_content" 
        android:layout_below="@+id/TextView10" 
        android:layout_centerHorizontal="true"
        android:id="@+id/Button01" 
        android:text="Exit" 
        android:layout_height="wrap_content"></Button>
</RelativeLayout>
